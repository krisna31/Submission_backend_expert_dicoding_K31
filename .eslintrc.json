{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "max-len": "off",
    "class-methods-use-this": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off",
    "no-unused-expressions": "off",
    "array-callback-return": "off"
  }
}
